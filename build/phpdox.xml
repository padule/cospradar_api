<?xml version="1.0" encoding="utf-8" ?>
<phpdox xmlns="http://phpdox.de/config">
    <project name="smape-h" source="cake/app" workdir="build/phpdox">
        <collector publiconly="false">
            <include mask="*.php" />
            <exclude mask="cake/app/Plugin/**" />
            <exclude mask="cake/app/Vendor/**" />
            <exclude mask="cake/lib/**" />
        </collector>

        <generator output="build">
            <build engine="html" enabled="true" output="api" />
        </generator>
    </project>
</phpdox>
